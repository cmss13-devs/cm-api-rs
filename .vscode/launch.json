{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug executable 'cm-api-rs'",
      "cargo": {
        "args": ["build", "--bin=cm-api-rs", "--package=cm-api-rs"],
        "filter": {
          "name": "cm-api-rs",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug unit tests in executable 'cm-api-rs'",
      "cargo": {
        "args": ["test", "--no-run", "--bin=cm-api-rs", "--package=cm-api-rs"],
        "filter": {
          "name": "cm-api-rs",
          "kind": "bin"
        }
      },
      "args": [],
      "cwd": "${workspaceFolder}"
    }
  ]
}
